pipeline{
agent any{
    steps{    
      echo "building the code";
      bat "mvn clean"
    }
 }
  
 stage('Test'){
    steps{    
      echo "Testing the code";
      bat "mvn test"
    }  
  }
  stage('Compile'){
    steps{    
      echo "Compiling the code";
      bat "mvn Compile"
    }  
  }   
  stage('Deploy'){
    steps{    
      echo "Deploy the code";
    }  
  }  
}  
}
